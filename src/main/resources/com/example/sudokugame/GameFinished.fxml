<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="250.0" prefWidth="350.0" style="-fx-background-color: #F4F6FF;" stylesheets="@css/sudoku.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sudokugame.ui.PopUpController">
    <children>
        <Label id="lblGameOver" layoutX="92.0" layoutY="49.0" prefHeight="58.0" prefWidth="167.0" text="VICTORY!" />
        <Button fx:id="menuBtn" layoutX="20.0" layoutY="157.0" mnemonicParsing="false" onAction="#switchToMenu" prefHeight="50.0" prefWidth="154.0" styleClass="gameOverButton" text="MENU" />
        <Button fx:id="newGameBtn" layoutX="176.0" layoutY="157.0" mnemonicParsing="false" onAction="#newGame" prefHeight="50.0" prefWidth="154.0" styleClass="gameOverButton" text="NEW GAME" />
        <Label id="instruction" layoutX="47.0" layoutY="108.0" prefHeight="26.0" prefWidth="252.0" text="You have passed this level!">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
